fn double(n: int) -> int {
    return n * 2;
}

fn add(a: int, b: int) -> int {
    return a + b;
}

print("Hello, Carv!");

let x = 10;
let y = 20;

x |> double |> print;
x |> double |> add(5) |> print;

if x < y {
    print("x is less than y");
}

for (let i = 0; i < 3; i = i + 1) {
    print("i =", i);
}

let nums = [1, 2, 3, 4, 5];
for n in nums {
    print(n);
}

print("array:", [1, 2, 3, 4, 5]);
print("length:", len([1, 2, 3, 4, 5]));
